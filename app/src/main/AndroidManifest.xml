            android:name=".Activity.RegisterActivity"
            android:theme="@style/Translucent" />
        <activity
            android:name=".Activity.PhoneConfirmActivity"
            android:theme="@style/Translucent" />
        <activity
            android:name=".Activity.stu_main"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name=".Activity.JingpincourseActivity"
            android:theme="@style/AppTheme_CUI" />
        <activity
            android:name=".Activity.FreecourseActivity"
            android:theme="@style/AppTheme_CUI" />
        <activity
            android:name=".Activity.AllcourseActivity"
            android:theme="@style/AppTheme_CUI" />
        <activity
            android:name=".Activity.ScheduleActivity"
            android:theme="@style/MyAppTheme" />
        <activity android:name=".Activity.EnglishCET6Activity" />
        <activity android:name=".Activity.EnglishSearchActivity" />
        <activity android:name=".Activity.EnglishCET4Activity" />
    </application>

</manifest>