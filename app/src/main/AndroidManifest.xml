                  android:label="我的成绩">

        </activity>
        <activity android:name=".Activity.MyClassActivity"
                  android:theme="@style/AppTheme_Kang"/>
        <activity android:name=".Activity.MaterialActivity"
                  android:label="考试安排"
                  android:theme="@style/AppTheme_Kang"/>
        <activity android:name=".Activity.KechengActivity"
                  android:label="我的课程"
                  android:theme="@style/AppTheme_Kang"/>
        <activity android:name=".Activity.ExamArrangementActivity" />
        <activity
            android:name=".Activity.LoginActivity"
            android:theme="@style/LaunchScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.TuijianActivity"
            android:theme="@style/AppTheme_CUI" />
